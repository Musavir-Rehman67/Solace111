        android:textSize="30sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="45dp"
        android:background="#666666"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/spinnerPosThnk"
            android:layout_width="17sp"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginStart="9dp"
            android:layout_marginTop="45dp"/>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/button1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:width="20dp"
            app:backgroundTint="@color/sol"
            android:layout_marginStart="-45dp"
            android:layout_marginTop="43dp"
            android:background="@drawable/ic_baseline_arrow_right_24" />
        <CheckBox
            android:id="@+id/positivethinking"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="-20dp"
            android:padding="5dp"
            android:layout_marginTop="50dp"
            android:text="Positive Thinking"
            android:textSize="20sp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/spinnertherapies"
            android:layout_width="17sp"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="100dp"/>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:width="20dp"
            app:backgroundTint="@color/sol"
            android:layout_marginStart="-18dp"
            android:layout_marginTop="-38dp"
            android:background="@drawable/ic_baseline_arrow_right_24" />
        <CheckBox
            android:id="@+id/therapies"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:padding="5dp"
            android:layout_marginTop="-43dp"
            android:text="Alternative Therapies"
            android:textSize="20sp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/spinnerRelax"
            android:layout_width="17sp"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginStart="10dp"
