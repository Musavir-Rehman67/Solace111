            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:width="20dp"
            app:backgroundTint="@color/sol"
            android:layout_marginStart="-18dp"
            android:layout_marginTop="-38dp"
            android:background="@drawable/ic_baseline_arrow_right_24" />
        <CheckBox
            android:id="@+id/relaxation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:padding="5dp"
            android:layout_marginTop="-43dp"
            android:text="Relaxation"
            android:textSize="20sp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/spinnerPlenty"
            android:layout_width="17sp"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="180dp"/>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:width="20dp"
            app:backgroundTint="@color/sol"
            android:layout_marginStart="-18dp"
            android:layout_marginTop="-38dp"
            android:background="@drawable/ic_baseline_arrow_right_24" />
        <CheckBox
            android:id="@+id/powater"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:padding="5dp"
            android:layout_marginTop="-43dp"
            android:text="Plenty Of Water"
            android:textSize="20sp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/spinnerAdequate"
            android:layout_width="17sp"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginStart="10dp"
